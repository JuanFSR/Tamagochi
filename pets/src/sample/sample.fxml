<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button fx:id="btt_msg" layoutX="270.0" layoutY="345.0" mnemonicParsing="false" onAction="#pressButton" text="Cadastrar">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <MenuBar layoutY="2.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <menus>
                <Menu mnemonicParsing="false" text="Telas">
                  <items>
                      <MenuItem fx:id="bttJanelaListaP" mnemonicParsing="false" onAction="#bttJanelaListaPClick" text="Listar Pets" />
                      <MenuItem fx:id="bttJanelaCadastro" mnemonicParsing="false" onAction="#bttJanelaCadastroClick" text="Cadastrar Pet" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <ImageView fitHeight="103.0" fitWidth="106.0" layoutX="249.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@imagens/logo.jpg" />
               </image></ImageView>
            <Label layoutX="245.0" layoutY="154.0" prefHeight="27.0" prefWidth="112.0" text="Cadastrar Pet">
               <font>
                  <Font name="Bookshelf Symbol 7" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="txtNomePet" layoutX="181.0" layoutY="209.0" prefHeight="25.0" prefWidth="241.0" />
            <Label layoutX="185.0" layoutY="192.0" text="Nome do Pet" />
            <Label layoutX="185.0" layoutY="255.0" prefHeight="17.0" prefWidth="70.0" text="Cor do Pet" />
            <Button fx:id="bttCorPetAzul" layoutX="185.0" layoutY="287.0" mnemonicParsing="false" onAction="#bttClickCorPetAzul" text="Azul" />
            <Button fx:id="bttCorPetVermelho" layoutX="244.0" layoutY="287.0" mnemonicParsing="false" onAction="#bttClickCorPetVermelho" text="Vermelho" />
            <Button fx:id="bttCorPetAmarelo" layoutX="331.0" layoutY="287.0" mnemonicParsing="false" onAction="#bttClickCorPetAmarelo" text="Amarelo" />
            <Button fx:id="bttCorPetVerde" layoutX="414.0" layoutY="287.0" mnemonicParsing="false" onAction="#bttClickCorPetVerde" text="Verde" />
         </children>
      </AnchorPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
